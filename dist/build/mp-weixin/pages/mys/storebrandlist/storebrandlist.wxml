<view><view class="card_list"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="inex" wx:key="*this"><view class="card_item"><view class="left"><view class="top"><view>{{item.brand_name}}</view><view>申请人电话:</view></view><view class="bottom"><view>{{item.shop_name}}</view><view>{{item.mobile}}</view></view></view><view class="right"><block wx:if="{{item.status=='0'}}"><view class="btn"><button class="yes btn_public" data-id="{{item.id}}" data-btn="1" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">同意</button><button class="no btn_public" data-id="{{item.id}}" data-btn="2" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">驳回</button></view></block><block wx:else><view class="txt"><text>{{item.status=='1'?"已审核":"不通过"}}</text></view></block></view></view></block></view><uni-load-more vue-id="1" status="{{noData}}" bind:__l="__l"></uni-load-more></view>